                <button type="button" class="close" data-dismiss="modal" id="closepopup"></button>
                <h4 class="modal-title">Document Generator</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-xs-12">
                        <div style="width:345px;" class="col-xs-4 doctypegen-leftcol">
                            @*<div class="col-xs-12 doctypegen-heading">Documents</div>*@
                            <div class="doctype-search-wrapper">
                                <div class="center-align input-type-search-icon input-icon">
                                    <input id="searchDocumentType" class="col-xs-12 search-text-box" type="text" size="50" placeholder="Search document" />
                                </div>
                            </div>
                            @Html.Partial("_DocumentTypes", Model)
                        </div>
                        <div id="updateContainer">
                           @* @Html.Partial("_DocumentGeneratorForm", new DocumentGeneratorViewModel() {  Drivers= new List<SelectListItem>(), Title = "", Billings = new EZRAC.Risk.UI.Web.ViewModels.Claims.BillingViewModel() {  Billings = new List<EZRAC.Risk.UI.Web.ViewModels.Claims.RiskBillingsViewModel>()} })*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

